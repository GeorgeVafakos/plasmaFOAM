FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      volt;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    leftPlateHe
    {
        type            fixedValue;
        value           uniform 0;
    }

    rightPlateHe
    {
        type            fixedValue;
        value           uniform 0;
    }

    region0_to_dielectric
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            voltD;
        kappaMethod     lookup;
        kappa		    epsilon;
    }

}

