FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      Fc;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

    upperBoundary
    {
        type            zeroGradient;
    }

    leftBoundary
    {
        type            zeroGradient;
    }

    rightBoundary
    {
        type            zeroGradient;
    }

    highVoltage
    {
        type            zeroGradient;
    }

    region0_to_dielectric
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            symmetry;
    }

}

