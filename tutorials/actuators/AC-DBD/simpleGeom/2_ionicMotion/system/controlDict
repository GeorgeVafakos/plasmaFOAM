FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    object          controlDict;
}

application     actuatorACDBDIonicFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1.0e-4;

deltaT          1e-09;

writeControl    adjustableRunTime;

writeInterval   0.0001;

purgeWrite      0;

writeFormat     ascii;

writePrecision  14;

writeCompression off;

timeFormat      general;

timePrecision   10;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.8;

maxDeltaT       1;

writeCustomTime true;

printScreenResults 100;

maxRegionLoopsIndEqn 1;

numWriteTimes   5;

P               0.1;

Q               5;

functions
{    
    #include "probes"
    
    fieldAverage1
    {
        type            fieldAverage;
        libs            (fieldFunctionObjects);
        writeControl    writeTime;
        fields          ( Fc { mean on ; prime2Mean off ; base time ; } );
        log             false;
    }
}


