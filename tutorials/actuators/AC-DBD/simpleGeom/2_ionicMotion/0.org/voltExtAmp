FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      voltExtAmp;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

voltage         uniform -10.0e3;

boundaryField
{

    upperBoundary
    {
        type            zeroGradient;
    }

    leftBoundary
    {
        type            zeroGradient;
    }

    rightBoundary
    {
        type            zeroGradient;
    }

    highVoltage
    {
        type            fixedValue;
        value           $voltage;
    }

    region0_to_dielectric
    {
        type            compressible::turbulentTemperatureRadCoupledMixed;
        value           $internalField;
        Tnbr            voltExtAmp;
        kappaMethod     lookup;
        kappa           epsilon;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }

}

// ************************************************************************* //
