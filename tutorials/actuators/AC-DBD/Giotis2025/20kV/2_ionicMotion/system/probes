FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      probes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

probes
{
    name    probes;
    type    probes;

    libs    (sampling);

    writeControl   writeTime;
    writeInterval  1;

    fields  (rhoq Fc);

    probeLocations
    (
        (1.0e-3 0.0105 0)
    );

}


