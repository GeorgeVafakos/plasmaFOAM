FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

    upperBoundary
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }

    leftBoundary
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }

    rightBoundary
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }

    region0_to_dielectric
    {
        type            noSlip;
    }

    highVoltage
    {
        type            noSlip;
    }

    front
    {
        type            wedge;
    }

    back
    {
        type            wedge;
    }
}

