FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    HeInlet
    {
        type            parabolicInlet;
        meanVelocity    (0 0 17.68);
        center          (0 0 0);
    }   

    airInlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }

    farfield
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }
}


