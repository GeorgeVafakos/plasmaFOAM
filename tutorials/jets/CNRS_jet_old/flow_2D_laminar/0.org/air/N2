FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      N2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.8;

boundaryField
{
    airInlet
    {
        type            fixedValue;
        value           $internalField;
    }

    air_to_region0
    {
        type            fixedValue;
        value           $internalField;
    }

    air_to_dielectric
    {
        type            fixedValue;
        value           $internalField;
    }

    topBoundary
    {
        type            fixedValue;
        value           $internalField;
    }

    frontWedge
    {
        type            wedge;
    }

    backWedge
    {
        type            wedge;
    }
}


