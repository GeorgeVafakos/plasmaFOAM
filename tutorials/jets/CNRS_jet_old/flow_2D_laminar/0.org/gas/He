FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      He;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    electrode
    {
        type            fixedValue;
        value           $internalField;
    }

    gasInlet
    {
        type            fixedValue;
        value           $internalField;
    }

    gas_to_dielectric
    {
        type            fixedValue;
        value           $internalField;
    }

    gas_to_region0
    {
        type            fixedValue;
        value           $internalField;
    }

    axis
    {
        type            empty;
    }

    frontWedge
    {
        type            wedge;
    }

    backWedge
    {
        type            wedge;
    }
}


