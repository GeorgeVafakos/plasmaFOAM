FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    region0_to_gas
    {
        type            parabolicInlet;
        meanVelocity    (0 0 17.68);
        center          (0 0 7.0e-3);
    }

    region0_to_air
    {
        type            zeroGradient;
    }

    region0_to_dielectric
    {
        type            noSlip;
    }

    outlet
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }

    topBoundary
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }

    axis
    {
        type            empty;
    }

    frontWedge
    {
        type            wedge;
    }

    backWedge
    {
        type            wedge;
    }
}


