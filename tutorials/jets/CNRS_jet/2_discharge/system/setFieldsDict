FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue He 0.0
    volScalarFieldValue N2 0.0
    volScalarFieldValue O2 0.0
);

regions
(
    cylinderToCell
    {
        p1 (0 0 -3.0e-3);
        p2 (0 0 7.0e-3);
        radius 0.3e-3;
        fieldValues
        (
            volScalarFieldValue He 1.0
        );
    }

    cylinderAnnulusToCell
    {
        p1 (0 0 -3.0e-3);
        p2 (0 0 7.0e-3);
        innerRadius 0.5e-3;
        radius 2.5e-3;
        fieldValues
        (
            volScalarFieldValue N2 0.8
            volScalarFieldValue O2 0.2
        );
    }
);


