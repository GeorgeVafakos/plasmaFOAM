FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      He;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    gasInlet
    {
        type            fixedValue;
        value           uniform 1;
    }

    airInlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    region0_to_dielectric
    {
        type            zeroGradient;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    topBoundary
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    axis
    {
        type            empty;
    }

    frontWedge
    {
        type            wedge;
    }

    backWedge
    {
        type            wedge;
    }
}


// ************************************************************************* //
