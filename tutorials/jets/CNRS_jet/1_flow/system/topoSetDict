FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    // Tube's exit
    {
        name    tubeExitSet;
        type    faceSet;
        action  new;
        source  cylinderToFace;
        sourceInfo
        {
            p1 (0.0 0.0 6.99e-3);
            p2 (0.0 0.0 7.01e-3);
            radius 0.3e-3;
        }
    }
    {
        name    tubeExitZone;
        type    faceZoneSet;
        action  new;
        source  setToFaceZone;
        sourceInfo
        {
            faceSet tubeExitSet;
        }
    }
);

