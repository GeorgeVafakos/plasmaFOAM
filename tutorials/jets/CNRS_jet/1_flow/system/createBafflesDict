FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      createBafflesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

internalFacesOnly true;

baffles
{
    tubeExitBaffle
    {
        //- Use predefined faceZone to select faces and orientation.
        type        faceZone;
        zoneName    tubeExitZone;

        patches
        {
            master
            {
                name        tubeExit;
                type        patch;
            }
            slave
            {
                $master;
            }
        }
    }
}


