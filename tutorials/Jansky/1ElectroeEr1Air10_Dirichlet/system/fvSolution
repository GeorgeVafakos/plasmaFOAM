FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    volt
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-10;
        // relTol          0.2;
    }

    voltD
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-9;
        // relTol          0.2;
    }

    voltI
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-9;
        // relTol          0.2;
    }
}


