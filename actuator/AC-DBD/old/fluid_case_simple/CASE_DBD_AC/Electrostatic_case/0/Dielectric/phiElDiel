FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/Dielectric";
    object      phiElDiel;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 2 -3 0 0 -1 0 ];

internalField   uniform 0;

boundaryField
{
    LeftWall
    {
        type            fixedValue;
        value           uniform 0;
    }
    RightWall
    {
        type            fixedValue;
        value           uniform 0;
    }
    DownWall
    {
        type            fixedValue;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
    Dielectric_to_Air
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            phiEl;
        kappaMethod     lookup;
        kappa           k_Diel;
    }
    anode
    {
        type            fixedValue;
        value           uniform 20000;
    }
    cathode
    {
        type            fixedValue;
        value           uniform 0;
    }
}


