FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    upperBoundary
    {
        type            calculated;
        value           uniform 0;
    }

    leftBoundary
    {
        type            calculated;
        value           uniform 0;
    }

    rightBoundary
    {
        type            calculated;
        value           uniform 0;
    }

    region0_to_dielectric
    {
        type            nutkWallFunction;
        value           uniform 0;
    }

    highVoltage
    {
        type            nutkWallFunction;
        value           uniform 0;
    }

    frontAndBack
    {
        type            empty;
    }
}

