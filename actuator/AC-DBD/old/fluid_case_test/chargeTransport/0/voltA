FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      volt;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{

    upperBoundary
    {
        type            fixedValue;
        value           uniform 0;
    }

    leftBoundary
    {
        type            fixedValue;
        value           uniform 0;
    }

    rightBoundary
    {
        type            fixedValue;
        value           uniform 0;
    }

    highVoltage
    {
        type            uniformFixedValue;
        uniformValue
        {
            type             sine;
            frequency        1.0e+3;
            amplitude        10.0e+3;
            scale            1;  // Scale factor for wave
            level            0;  // Offset
        }
    }

    region0_to_dielectric
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            voltD;
        kappaMethod     lookup;
        kappa           epsilonA;
    }

    frontAndBack
    {
        type            empty;
    }

}

// ************************************************************************* //
