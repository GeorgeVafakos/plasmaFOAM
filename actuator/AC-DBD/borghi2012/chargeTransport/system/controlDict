FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "system";
    object          controlDict;
}

application     actuatorACDBDChargeFoam;

startFrom       startTime;

startTime       1e-05;

stopAt          endTime;

endTime         0.0002;

deltaT          1e-10;

writeControl    adjustableRunTime;

writeInterval   0.0001;

purgeWrite      0;

writeFormat     ascii;

writePrecision  14;

writeCompression off;

timeFormat      general;

timePrecision   10;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.4;

maxDeltaT       1;

writeCustomTime yes;

numWriteTimes   100;

P               0.1;

Q               5;

numStreamers    5;

lengthStreamer  8.6e-06;

functions
{
    probes
    {
        functionObjectLibs ( "libsampling.so" );
        type            probes;
        name            probes;
        writeControl    timeStep;
        writeInterval   1;
        fields          ( voltA rhoq Fc );
        probeLocations  ( ( 0 4e-05 0.025 ) ( 0.0015 4e-05 0.025 ) ( 0.003 4e-05 0.025 ) ( 0.0045 4e-05 0.025 ) );
    }
}


// ************************************************************************* //
