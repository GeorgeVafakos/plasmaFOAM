FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 5;

boundaryField
{
    upperBoundary
    {
        type            zeroGradient;
    }

    leftBoundary
    {
        type            zeroGradient;
    }

    rightBoundary
    {
        type            zeroGradient;
    }

    region0_to_dielectric
    {
        type            epsilonWallFunction;
        value           uniform 5; //5
    }

    highVoltage
    {
        type            epsilonWallFunction;
        value           uniform 5;
    }

    frontAndBack
    {
        type            empty;
    }
}


