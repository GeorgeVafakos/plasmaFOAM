FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      Fc;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

    upperBoundary
    {
        type            fixedValue;
        value           (0 0 0);
    }

    leftBoundary
    {
        type            fixedValue;
        value           (0 0 0);
    }

    rightBoundary
    {
        type            fixedValue;
        value           (0 0 0);
    }

    highVoltage
    {
        type            fixedValue;
        value           (0 0 0);
    }

    region0_to_dielectric
    {
        type            fixedValue;
        value           (0 0 0);
    }

    frontAndBack
    {
        type            empty;
    }

}

