FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    object          controlDict;
}

application     actuatorACDBDIonicFoam;

startFrom       startTime;

startTime       5e-05;

stopAt          endTime;

endTime         0.0001;

deltaT          1e-09;

writeControl    adjustableRunTime;

writeInterval   0.0001;

purgeWrite      0;

writeFormat     ascii;

writePrecision  14;

writeCompression off;

timeFormat      general;

timePrecision   10;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.8;

maxDeltaT       1;

writeCustomTime yes;

numWriteTimes   50;

P               0.1;

Q               5;

numDischarges    14;

timeBetweenDischarges  2.5e-06;

functions
{
    probes
    {
        libs            (fieldFunctionObjects);
        type            probes;
        name            probes;
        writeControl    timeStep;
        writeInterval   1;
        fields          ( volt rhoq Fc );
        probeLocations  ( ( 0 4e-05 0.025 ) ( 0.0015 4e-05 0.025 ) ( 0.003 4e-05 0.025 ) ( 0.0045 4e-05 0.025 ) );
    }
    fieldAverage1
    {
        type            fieldAverage;
        libs            ( "libfieldFunctionObjects.so" );
        writeControl    writeTime;
        fields          ( Fc { mean on ; prime2Mean on ; base time ; } );
    }
}


