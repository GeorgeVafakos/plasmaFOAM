FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0.0001/encapsulator";
    object      E;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 0 0 -1 0];

internalField   uniform (-0 -0 -0);

boundaryField
{
    leftBoundary
    {
        type            extrapolatedCalculated;
        value           uniform (-0 -0 -0);
    }
    rightBoundary
    {
        type            extrapolatedCalculated;
        value           uniform (-0 -0 -0);
    }
    ground
    {
        type            extrapolatedCalculated;
        value           uniform (-0 -0 -0);
    }
    bottomBoundary
    {
        type            extrapolatedCalculated;
        value           uniform (-0 -0 -0);
    }
    frontAndBack
    {
        type            empty;
    }
    encapsulator_to_dielectric
    {
        type            extrapolatedCalculated;
        value           uniform (-0 -0 -0);
    }
}


