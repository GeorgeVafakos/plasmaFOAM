FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "8e-05/encapsulator";
    object      volt;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    leftBoundary
    {
        type            calculated;
        value           uniform 0;
    }
    rightBoundary
    {
        type            calculated;
        value           uniform 0;
    }
    ground
    {
        type            calculated;
        value           uniform 0;
    }
    bottomBoundary
    {
        type            calculated;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
    encapsulator_to_dielectric
    {
        type            calculated;
        value           uniform 0;
    }
}


