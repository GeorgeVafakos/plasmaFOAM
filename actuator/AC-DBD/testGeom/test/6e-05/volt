FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "6e-05";
    object      volt;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    upperBoundary
    {
        type            calculated;
        value           uniform 0;
    }
    leftBoundary
    {
        type            calculated;
        value           uniform 0;
    }
    rightBoundary
    {
        type            calculated;
        value           uniform 0;
    }
    highVoltage
    {
        type            calculated;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
    region0_to_dielectric
    {
        type            calculated;
        value           uniform 0;
    }
}


