FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      Fc;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    upperBoundary
    {
        type            zeroGradient;
    }
    leftBoundary
    {
        type            zeroGradient;
    }
    rightBoundary
    {
        type            zeroGradient;
    }
    highVoltage
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
    region0_to_dielectric
    {
        type            zeroGradient;
    }
}


