FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    // High Voltage electrode
    {
        name    HVelectrodeSet;
        type    faceSet;
        action  new;
        source  boxToFace;
        sourceInfo
        {
            box (-5.0e-3 0.0 0) (0.0 0.0 0.05);
        }
    }
    {
        name    HVelectrodeZone;
        type    faceZoneSet;
        action  new;
        source  setToFaceZone;
        sourceInfo
        {
            faceSet HVelectrodeSet;
        }
    }

    // Grounded Electrode
    {
        name    GRelectrodeSet;
        type    faceSet;
        action  new;
        source  boxToFace;
        sourceInfo
        {
            box (3.0e-3 -0.425001e-3 0) (19.0e-3 -0.424999E-3 0.05);
        }
    }
    {
        name    GRelectrodeZone;
        type    faceZoneSet;
        action  new;
        source  setToFaceZone;
        sourceInfo
        {
            faceSet GRelectrodeSet;
        }
    }
);

