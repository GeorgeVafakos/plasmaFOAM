FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    voltA
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-06;
        //relTol          0.05;
        //maxIter         1;      
    }
    
    voltD
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-06;
        //relTol          0.05;
        //maxIter         1;      
    }

    voltI
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-06;
        //relTol          0.05;
        //maxIter         1;      
    }


    rhoq
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-8;
        relTol          0.05;
        maxIter         0;      // This line suppresses the charge transport equation
    }
}


