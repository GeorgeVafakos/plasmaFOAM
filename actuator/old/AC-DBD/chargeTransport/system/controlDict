FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "system";
    object          controlDict;
}

application     actuatorElectrostaticChargeFoam;

startFrom       startTime;

startTime       1.21872e-05;

stopAt          endTime;

endTime         0.0001;

deltaT          1e-07;

writeControl    adjustableRunTime;

writeInterval   1e-06;

purgeWrite      0;

writeFormat     ascii;

writePrecision  14;

writeCompression off;

timeFormat      general;

timePrecision   14;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.8;

maxDeltaT       1;

writeCustomTime yes;

numWriteTimes   15;

P               0.1;

Q               5.0;

functions
{
    probes
    {
        functionObjectLibs ( "libsampling.so" );
        type            probes;
        name            probes;
        writeControl    timeStep;
        writeInterval   1;
        fields          ( voltA rhoq Fc );
        probeLocations  ( ( 0 5e-05 0.025 ) ( 0.0015 5e-05 0.025 ) ( 0.003 5e-05 0.025 ) ( 0.0045 5e-05 0.025 ) );
    }
}


// ************************************************************************* //
