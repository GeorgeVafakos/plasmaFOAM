FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      voltI;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    leftBoundary
    {
        type            zeroGradient;
    }

    rightBoundary
    {
        type            zeroGradient;
    }

    bottomBoundary
    {
        type            zeroGradient;
    }

    ground
    {
        type            fixedValue;
        value           uniform 0;
    }

    insulator_to_dielectric
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            voltD;
        kappaMethod     lookup;
        kappa           epsilonI;
    }

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
