FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue rhoq 0.0
);

regions
(
    boxToCell
    {
        box (0.0 0.0 -1.0) (50e-6 0.000040 1.0);
        fieldValues
        (
            volScalarFieldValue rhoq 0.1
        );
    }
);


