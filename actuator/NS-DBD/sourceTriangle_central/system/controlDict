FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     actuatorNSrhoCentralFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         2e-05;

deltaT          1e-09;

writeControl    adjustableRunTime;

writeInterval   1.0e-7;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  no;

maxCo           0.4;

maxDeltaT       1.0e-8;

Period          5.0e-9;

HeatingTime     2.0e-9;

startHeating    0.0;

functions
{
    #includeFunc mag(U)
}

