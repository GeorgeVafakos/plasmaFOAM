FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     actuatorNSDBDRhoPimpleFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1e-05;

deltaT          1e-09;

writeControl    adjustableRunTime;

writeInterval   1e-07;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{
    #includeFunc mag(U)
}

