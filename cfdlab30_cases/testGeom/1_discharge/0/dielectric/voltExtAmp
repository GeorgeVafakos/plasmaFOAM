/*--------------------------------*- C++ -*----------------------------------*\
  =========                 |
  \\      /  F ield         | OpenFOAM: The Open Source CFD Toolbox
   \\    /   O peration     | Website:  https://openfoam.org
    \\  /    A nd           | Version:  7  
     \\/     M anipulation  |
\*---------------------------------------------------------------------------*/
FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      voltExtAmp;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

voltage         uniform -10.0e3;

boundaryField
{
    leftBoundary
    {
        type            zeroGradient;
    }

    rightBoundary
    {
        type            zeroGradient;
    }

    highVoltage
    {
        type            fixedValue;
        value           $voltage;
    }
    
    ground
    {
        type            fixedValue;
        value           uniform 0;
    }

    dielectric_to_region0
    {
        type            compressible::turbulentTemperatureRadCoupledMixed;
        value           $internalField;
        Tnbr            voltExtAmp;
        kappaMethod     lookup;
        kappa           epsilon;
    }

    dielectric_to_encapsulator
    {
        type            compressible::turbulentTemperatureRadCoupledMixed;
        value           $internalField;
        Tnbr            voltExtAmp;
        kappaMethod     lookup;
        kappa           epsilon;
    }

    frontAndBack
    {
        type            empty;
    }

}

// ************************************************************************* //
