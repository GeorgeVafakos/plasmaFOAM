FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "4e-13";
    object      EArho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 0 0 -1 0];

internalField   uniform (-0 -0 -0);

boundaryField
{
    upperBoundary
    {
        type            extrapolatedCalculated;
        value           uniform (-0 -0 -0);
    }
    leftBoundary
    {
        type            extrapolatedCalculated;
        value           nonuniform 0();
    }
    rightBoundary
    {
        type            extrapolatedCalculated;
        value           uniform (-0 -0 -0);
    }
    frontAndBack
    {
        type            empty;
    }
    highVoltage
    {
        type            extrapolatedCalculated;
        value           nonuniform 0();
    }
    region0_to_dielectric
    {
        type            extrapolatedCalculated;
        value           uniform (-0 -0 -0);
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (-0 -0 -0);
    }
}


// ************************************************************************* //
