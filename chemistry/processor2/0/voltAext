FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      voltAext;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    upperBoundary
    {
        type            zeroGradient;
    }
    leftBoundary
    {
        type            zeroGradient;
    }
    rightBoundary
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
    highVoltage
    {
        type            fixedValue;
        value           uniform 1200;
    }
    region0_to_dielectric
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        Tnbr            voltDext;
        kappaMethod     lookup;
        kappa           epsilonA;
        value           uniform 0;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 0;
    }
}


