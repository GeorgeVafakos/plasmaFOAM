FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      ne;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];

internalField   uniform 1e+13;

boundaryField
{
    upperBoundary
    {
        type            zeroGradient;
    }
    leftBoundary
    {
        type            zeroGradient;
    }
    rightBoundary
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
    highVoltage
    {
        type            zeroGradient;
    }
    region0_to_dielectric
    {
        type            zeroGradient;
    }
    procBoundary3to0
    {
        type            processor;
        value           uniform 1e+13;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 1e+13;
    }
}


