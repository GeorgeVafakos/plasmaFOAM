FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "5e-13/dielectric";
    object      EDrho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 0 0 -1 0];

internalField   uniform (-0 -0 -0);

boundaryField
{
    leftBoundary
    {
        type            extrapolatedCalculated;
        value           nonuniform 0();
    }
    rightBoundary
    {
        type            extrapolatedCalculated;
        value           nonuniform 0();
    }
    ground
    {
        type            extrapolatedCalculated;
        value           uniform (-0 -0 -0);
    }
    frontAndBack
    {
        type            empty;
    }
    highVoltage
    {
        type            extrapolatedCalculated;
        value           nonuniform 0();
    }
    dielectric_to_region0
    {
        type            extrapolatedCalculated;
        value           uniform (-0 -0 -0);
    }
    procBoundary3to0
    {
        type            processor;
        value           uniform (-0 -0 -0);
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform (-0 -0 -0);
    }
}


